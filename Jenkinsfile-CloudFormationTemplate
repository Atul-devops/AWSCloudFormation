pipeline
{
	agent any
	stages
	{
		stage ('My first cloud formation template to launch ec2 instance.')
		{
			steps
			{
				withAWS(credentials: 'awsrootaccesskey') {
				aws cloudformation create-stack --stack-name CloudFormationEC2instance --template-body file:///Jenkinsfile-CloudFormationTemplate.json
				}
			}
		}
	}
}
